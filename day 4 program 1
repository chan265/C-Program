#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() 
{
    char str[50];
    scanf("%s", str); 
    int len = strlen(str);
    for(int i = len - 1; i >= 0; i--) 
    {
        printf("%c", str[i]);
    }
    printf("\n");
    return 0;
}


