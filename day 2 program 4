

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char a[50];
    int b,c;
    scanf("%s%d%d",a,&b,&c);
    printf("Name of the Student:%s\n",a);
    if((b==1 && c>=70) || ((b==1 || b==2) && c>=75))
    {
        printf("%s is Eligible for Placement\n",a);
    }
    else
    {
        printf("%s is Not Eligible for Placement",a);
    }
    return 0;
}
