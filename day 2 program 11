Time

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
     int h,m,s;
    scanf("%d %d %d", &h, &m, &s);
    m += s / 60;
    s = s % 60;
    h += m / 60;
    m = m % 60;
    printf("Total Number of hours is %d\n", h);
    printf("Total Number of minutes is %d\n", m);
    printf("Total Number of seconds is %d\n", s);

    return 0;
}
