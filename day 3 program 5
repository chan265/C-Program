

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int n;
    scanf("%d", &n);
    int arr[n];
    float sum = 0, product = 1;
    for(int i = 0; i < n; i++) 
    {
        scanf("%d", &arr[i]);
        sum += arr[i];
        product *= arr[i];
    }
    float mean = sum / n;
    printf("Product of the array:%.2f\n", product);
    printf("Mean of the array:%.3f\n", mean);
    return 0;
}
