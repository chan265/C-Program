BMI Calculation

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() 
{
    float w,h;
    scanf("%f %f", &w, &h);
    h = h/100;
    float b = w/(h*h);
    if(b<18.5)
    {
        printf("You are underweight. Have an apple daily.");
    }
    else if(b>=18.5 && b<=24.9)
    {
        printf("You are normal. Go walking daily and maintain it.");
    }
    else if(b>=25 && b<=30)
    {
        printf("You are overweight. Go to Gym daily.");
    }
    else
    {
        printf("You are obese. You need to see a doctor.");
    }
    return 0;
}
