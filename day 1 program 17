

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() 
{
    int p,r,y;
    float i,a,d,f;  
    scanf("%d%d%d",&p,&r,&y);
    i=(p*r*y)/100.0;
    a=p+i;
    d=0.02*i;
    f=a-d;
    printf("%.2f\n",i);
    printf("%.2f\n",a);
    printf("%.2f\n",d);
    printf("%.2f\n",f);
    return 0;
}
