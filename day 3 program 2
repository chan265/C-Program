

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int n;
    scanf("%d", &n);
    float product = 1, num;
    for(int i = 0; i < n; i++) 
    {
        scanf("%f", &num);
        product *= num;
    }
    printf("The Product of list value is %.2f\n", product);
    return 0;
}
